{
    "name": "notfloran/mjml-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for MJML",
    "keywords": ["mjml", "email"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NotFloran\\MjmlBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotFloran\\MjmlBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Floran Brutel",
            "email": "me@floran.fr"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "symfony/process": "^3.4 || ^4.0 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^7.0",
        "matthiasnoback/symfony-config-test": "^4.1",
        "symplify/easy-coding-standard": "^7.2"
    },
    "suggest": {
        "swiftmailer/swiftmailer": "Plugin to integrate MJML in SwiftMailer"
    },
    "scripts": {
        "cs-check": "vendor/bin/ecs check tests src",
        "cs-fix": "vendor/bin/ecs check tests src --fix"
    }
}
